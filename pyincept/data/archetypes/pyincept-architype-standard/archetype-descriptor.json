{
  "directories": [
    {
      "subpath": "docs"
    },
    {
      "subpath": "scripts"
    },
    {
      "subpath": "{{package_name}}"
    },
    {
      "subpath": "tests/end_to_end/test_{{package_name}}"
    },
    {
      "subpath": "tests/integration/test_{{package_name}}"
    },
    {
      "subpath": "tests/unit/test_{{package_name}}"
    }
  ],
  "files": [
    {
      "subpath": "LICENSE",
      "prototype": "LICENSE.apache.jinja"
    },
    {
      "subpath": "log.cfg",
      "prototype": "log.cfg.jinja"
    },
    {
      "subpath": "Makefile",
      "prototype": "Makefile.jinja"
    },
    {
      "subpath": "Pipfile",
      "prototype": "Pipfile.jinja"
    },
    {
      "subpath": "README.rst",
      "prototype": "README.rst.jinja"
    },
    {
      "subpath": "setup.cfg",
      "prototype": "setup.cfg.jinja"
    },
    {
      "subpath": "setup.py",
      "prototype": "setup.py.jinja"
    },
    {
      "subpath": "{{package_name}}/__init__.py",
      "prototype": "__init___package.py.jinja"
    },
    {
      "subpath": "{{package_name}}/{{package_name}}.py",
      "prototype": "entry_point.py.jinja"
    },
    {
      "subpath": "tests/__init__.py",
      "prototype": "__init___tests.py.jinja"
    },
    {
      "subpath": "tests/end_to_end/__init__.py",
      "prototype": "__init___tests_end_to_end.py.jinja"
    },
    {
      "subpath": "tests/end_to_end/test_{{package_name}}/__init__.py",
      "prototype": "__init___tests_end_to_end_package.py.jinja"
    },
    {
      "subpath": "tests/integration/__init__.py",
      "prototype": "__init___tests_integration.py.jinja"
    },
    {
      "subpath": "tests/integration/test_{{package_name}}/__init__.py",
      "prototype": "__init___tests_integration_package.py.jinja"
    },
    {
      "subpath": "tests/unit/__init__.py",
      "prototype": "__init___tests_unit.py.jinja"
    },
    {
      "subpath": "tests/unit/test_{{package_name}}/__init__.py",
      "prototype": "__init___tests_unit_package.py.jinja"
    }
  ]
}